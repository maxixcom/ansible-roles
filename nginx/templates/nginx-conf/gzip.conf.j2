gzip               on;
gzip_buffers       16 8k;
gzip_comp_level    6;
gzip_disable       "MSIE [1-6].(?!.*SV1)";
gzip_http_version  1.1;
gzip_min_length    1100;
gzip_proxied       any;
gzip_types 
    application/xml
    application/xml+rss 
    application/x-javascript 
    text/css 
    text/javascript
    text/plain 
    text/xml;
gzip_vary           on;
